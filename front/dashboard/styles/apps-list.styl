.apps-list
	.headline
		text-align right
		height 54px
		line-height 54px
		padding 20px 28px

		h2
			color white
			font-size 30px
			float left

	.apps-table
		margin-top 30px
		margin 20px 28px

		table
			width 100%

			td
				padding-right 20px
				padding-top 10px

			td:last-child
				padding-right 0

			.name
				font-size 18px

			.key
				color $color-dark-text
				text-align right
