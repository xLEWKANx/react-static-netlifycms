$color-dark-blue = #26243f
.code-tabs
  margin-top 50px
  background #e3e3ec

  .code-tabs-dots
    display block
    float left
    margin-top 19px
    margin-left 15px
    margin-right 15px

.code-tabs-nav
  height 50px
  background $color-dark-blue
  background-image: -webkit-gradient(linear, 0 0, 0 63, from(rgba(0, 0, 0, 0)), color-stop(0, #2c2b42), color-stop(0.793, #1d1d35), color-stop(0.825, #1b1d34), color-stop(1, #1b1b33), color-stop(1, rgba(0, 0, 0, 0)));
  background-image: -webkit-linear-gradient(rgba(0, 0, 0, 0), #2c2b42 0%, #1d1d35 79.3%, #1b1d34 82.5%, #1b1b33 100%, rgba(0, 0, 0, 0) 100%);
  background-image: -moz-linear-gradient(rgba(0, 0, 0, 0), #2c2b42 0%, #1d1d35 79.3%, #1b1d34 82.5%, #1b1b33 100%, rgba(0, 0, 0, 0) 100%);
  background-image: -o-linear-gradient(rgba(0, 0, 0, 0), #2c2b42 0%, #1d1d35 79.3%, #1b1d34 82.5%, #1b1b33 100%, rgba(0, 0, 0, 0) 100%);
  background-image: linear-gradient(rgba(0, 0, 0, 0), #2c2b42 0%, #1d1d35 79.3%, #1b1d34 82.5%, #1b1b33 100%, rgba(0, 0, 0, 0) 100%)

  .code-tabs-nav-item.active
    background #12112d
    background-image: -webkit-gradient(linear, 0 0, 0 57, from(rgba(0, 0, 0, 0)), color-stop(0.035, #1b1b33), color-stop(0.14, #1b1d34), to(#2a2b42));
    background-image: -webkit-linear-gradient(rgba(0, 0, 0, 0), #1b1b33 3.5%, #1b1d34 14%, #2a2b42);
    background-image: -moz-linear-gradient(rgba(0, 0, 0, 0), #1b1b33 3.5%, #1b1d34 14%, #2a2b42);
    background-image: -o-linear-gradient(rgba(0, 0, 0, 0), #1b1b33 3.5%, #1b1d34 14%, #2a2b42);
    background-image: linear-gradient(rgba(0, 0, 0, 0), #1b1b33 3.5%, #1b1d34 14%, #2a2b42)
    border thin solid #35324c
    padding 14px 32px

.code-tabs-subnav
  height 42px
  background #ccccde
  clear both

.code-tabs-nav:after
  clear both

.code-tabs-nav-item
  cursor pointer
  outline none
  border none
  padding 15px 32px
  font-size 16px
  font-weight bold
  float left
  background transparent
  color white
  border-right thin solid #35324c
  height 50px

.code-tabs-subnav-item
  padding 11px 26px
  background #ccccde
  color #19192f
  border-right none
  height 42px

.code-tabs-subnav-item.active
  background #e3e3ec
  color #515078

.choices
  width 392px
  font-size 20px
  color white
  text-align center
  cursor pointer
  margin-top 50px

  .choices-section
    margin-left 60px

  .choices-section:first-child
    margin-left 0

  > span:hover
  > span.active
    border-bottom thin solid #83eaff
    color #83eaff

pre
  font-family monospace,monospace
  line-height 18px
  font-size 1em

code
  font-family monospace,monospace
  line-height 18px
  font-size 1em
  .hljs a 
    color #a9a8cb


code *
  font-family: monospace,monospace
  line-height 18px
  font-size 1em

  
.dev
  color #a9a8cb
  .hljs a 
    color blue
  
  .table
    td
      border-top 1px solid #a9a8cb 

  p
    font-size 16px
    color #a9a8cb
  a
    color white
  code
    background-color #ddd
    padding 5px 10px
    border-radius 2px
  pre
    padding 5px 5px
    background-color #ddd
    border-radius 2px
  blockquote
    color white
    footer 
      color rgba(255,255,255,0.70)
  
  .table>tbody>tr>td, .table>tbody>tr>th, .table>tfoot>tr>td, .table>tfoot>tr>th, .table>thead>tr>td, .table>thead>tr>th
    padding 8px
    line-height 1.428571429
    vertical-align top
    border-top 1px solid rgba(255,255,255,0.15)

  .dev-container
    margin-right 20px
    margin-left 20px

  .dev-content
    padding-left 20px
    padding-right 20px
    
  .dev-affix
    background-color rgba(255,255,255,0.15)
    padding 10px
    margin-top 25px
    border-radius 4px
    
    .title
      margin-top 5px
      margin-bottom 5px
      padding 5px 10px
    
    li 
      a
        padding 5px 10px
      :hover
        background-color rgba(255,255,255,0.15)
      

  .dev-header-title
    margin-right 20px
    margin-left 20px
    margin-bottom 30px

  .dev-navbar
    background-color white
    border 0px
    border-radius 0
    .nav li a
      font-size 16px
      padding-left 20px
      padding-right 20px
        

  .dev-header
    .dev-header-image
      z-index -1
      position absolute
      height 300px
      width 100%
      margin-top -100px
      background-image  url(/img/home/home-bg.png)
      background-repeat repeat-x
    .list-group
      border-radius 0
      border-top 1px solid rgba(255,255,255,0.15)
      border-bottom 1px solid rgba(255,255,255,0.15)
      background-color transparent
      .active
        background-color rgba(255,255,255,0.15)
        color white
    .list-group-item
      padding-left 20px
      padding-right 20px
      font-size 16px
      border-width 0
      background-color transparent
      color #8382a3
    .list-group-item:hover
      border-radius 0
      background-color rgba(255,255,255,0.15)
      color white
    .list-group-item:first-child
      border-radius 0
    .list-group-item:last-child
      border-radius 0